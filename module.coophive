{
  "machine": {
    "gpu": 1,
    "cpu": 1000,
    "ram": 1000
  },
  "job": {
    "APIVersion": "V1beta1",
    "Spec": {
      "EngineSpec": {
        "Type": "docker",
        "Params": {
          "Entrypoint": null,
          "EnvironmentVariables": [
            "OUTPUT_DIR=/outputs",
            "HF_DATASETS_OFFLINE=0",
            "TRANSFORMERS_OFFLINE=0"
          ],
          "Image": "ghcr.io/playback-network/som:sha-aa7771a",
          "Parameters": [
            "/inputs",
            {{if .slider}}"{{ printf "%s" .slider }}"{{else}}"2.6"{{end}},
            {{if .mode}}{{ printf "%s" .mode }}{{else}}"Automatic"{{end}},
            {{if .alpha}}{{ printf "%s" .alpha }}{{else}}"0.1"{{end}},
            {{if .label_mode}}{{ printf "%s" .label_mode }}{{else}}"Number"{{end}},
            {{if .anno_mode}}{{ printf "%s" .anno_mode }}{{else}}"Mask Mark"{{end}}
          ],
          "WorkingDirectory": "/app"
        }
      },
      "PublisherSpec": {
        "Type": "ipfs"
      },
      "Docker": {},
      "Wasm": {
        "EntryModule": {}
      },
      "Resources": {
        "CPU": "3",
        "Memory": "12gb",
        "GPU": "1"
      },
      "Network": {
        "Type": "None"
      },
      "Timeout": 3600,
      "Inputs": [
        {
          "StorageSource": "urlDownload",
          "Name": "rawImages",
          "URL": {{if .url}}"{{.url}}"{{else}}"https://raw.githubusercontent.com/gradio-app/gradio/main/test/test_files/bus.png"{{end}},
          "Path": "/inputs"
        }
      ],
      "Outputs": [
        {
          "Name": "outputs",
          "Path": "/outputs"
        }
      ],
      "Deal": {
        "Concurrency": 1
      }
    }
  }
}
